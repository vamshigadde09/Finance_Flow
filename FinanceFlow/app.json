{
  "expo": {
    "name": "FinanceFlow",
    "slug": "FinanceFlow",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "financeflow",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "sdkVersion": "54.0.0",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vamshi_gadde_9.FinanceFlow",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to camera to take photos for transactions.",
        "NSContactsUsageDescription": "This app needs access to contacts to help you manage transactions with your contacts.",
        "NSPhotoLibraryUsageDescription": "This app needs access to photo library to select images for transactions."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.READ_CONTACTS",
        "android.permission.READ_PHONE_NUMBERS",
        "android.permission.READ_CONTACTS",
        "android.permission.READ_PHONE_NUMBERS"
      ],
      "edgeToEdgeEnabled": true,
      "package": "com.vamshi_gadde_9.FinanceFlow"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow FinanceFlow to access your contacts."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}